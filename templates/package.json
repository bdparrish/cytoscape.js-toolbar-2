{
  "name": "{{fullName}}",
  "version": "{{version}}",
  "description": "{{description}}",
  "main": "{{fullName}}.js",
  "author": {
    "name": "{{author}}",
    "email": "{{authorEmail}}"
  },
  "scripts": {
    "postpublish": "run-s gh-pages:demo gh-pages:deploy gh-pages:clean",
    "gh-pages:demo": "cpy demo.html . --rename=index.html",
    "gh-pages:deploy": "gh-pages -d .",
    "gh-pages:clean": "rimraf index.html",
    "lint": "eslint src",
    "build": "cross-env NODE_ENV=production webpack",
    "build:min": "cross-env NODE_ENV=production MIN=true webpack",
    "watch": "webpack --progress --watch",
    "dev": "webpack-dev-server --open",
    "test": "mocha"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/{{{ghProj}}}.git"
  },
  "keywords": [
    "cytoscape",
    "cytoscape-extension"
  ],
  "license": "{{license}}",
  "bugs": {
    "url": "https://github.com/{{{ghProj}}}/issues"
  },
  "homepage": "https://github.com/{{{ghProj}}}",
  "devDependencies": {
    "babel-core": "^6.24.1",
    "babel-loader": "^7.0.0",
    "babel-preset-env": "^1.5.1",
    "camelcase": "^4.1.0",
    "chai": "4.0.2",
    "cross-env": "^5.0.0",
    "eslint": "^3.9.1",
    "mocha": "3.4.2",
    "webpack": "^2.6.1",
    "webpack-dev-server": "^2.4.5",
    "cpy-cli": "^1.0.1",
    "npm-run-all": "^4.1.2",
    "gh-pages": "^1.0.0",
    "rimraf": "^2.6.2"
  },
  "peerDependencies": {
    "cytoscape": "{{cyVersion}}"
  },
  "dependencies": {}
}
